                "enum": [ 20, 45, 90, 175, 330, 600, 1000 ],
                "condition": "{{ format_list(ch_num, single_ended_modes, "in%s_type==%s") }}"
            },
            "in{{ ch_num }}_n_measurements": {
                "title": "Number of measurements",
                "description": "Total sampling time of channel is (Sampling time * Number of measurements). Channels are polled sequentially.",
                "group": "gg_in{{ ch_num }}_n",
                "order": 3,
                "address": "0x{{ "%x"|format(ch_num) }}405",
                "min": 1,
                "max": 50,
                "condition": "{{ format_list(ch_num, single_ended_modes, "in%s_type==%s") }}"
            },
            "in{{ ch_num }}_n_lowpass": {
                "title": "Lowpass filter time, ms",
                "group": "gg_in{{ ch_num }}_n",
                "order": 4,
                "address": "0x{{ "%x"|format(ch_num) }}407",
                "min": 0,
                "max": 65000,
                "condition": "{{ format_list(ch_num, single_ended_modes, "in%s_type==%s") }}"
            },
            "in{{ch_num}}_n_gain": {
                "title": "Gain",
                "group": "gg_in{{ ch_num }}_n",
                "order": 4,
                "address": "0x{{ "%x"|format(ch_num) }}40D",
                "enum": [0, 1, 2, 4],
                "enum_titles": ["auto"],
                "default": 0,
                "condition": "{{ format_list(ch_num, types_with_gain, "in%s_n_type==%s") }}&&({{ format_list(ch_num, single_ended_modes, "in%s_type==%s") }})"
            },
            "in{{ ch_num }}_n_min": {
                "title": "Minimum value",
                "group": "gg_in{{ ch_num }}_n",
                "order": 5,
                "address": "0x{{ "%x"|format(ch_num) }}409",
                "default": 0,
                "condition": "{{ format_list(ch_num, types_with_min_max, "in%s_n_type==%s") }}&&({{ format_list(ch_num, single_ended_modes, "in%s_type==%s") }})"
            },
            "in{{ ch_num }}_n_max": {
                "title": "Maximum value",
                "group": "gg_in{{ ch_num }}_n",
                "order": 6,
                "address": "0x{{ "%x"|format(ch_num) }}40B",
                "default": 10000,
                "condition": "{{ format_list(ch_num, types_with_min_max, "in%s_n_type==%s") }}&&({{ format_list(ch_num, single_ended_modes, "in%s_type==%s") }})"
            },
            {% endfor -%}
            "baud_rate": {
                "title": "Baud rate",
                "description": "baud_rate_description",
                "address": 110,
                "reg_type": "holding",
                "enum": [96, 192, 384, 576, 1152],
                "default": 96,
                "enum_titles": [
                    "9600",
                    "19200",
                    "38400",
                    "57600",
                    "115200"
                ],
                "group": "general",
                "order": 1
            }
