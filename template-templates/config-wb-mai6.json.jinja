            "in{{ ch_num }}_min": {
                "title": "Minimum value",
                "group": "g_in{{ ch_num }}",
                "order": 5,
                "address": "0x{{ "%x"|format(ch_num) }}408",
                "default": 0,
                "condition": "{{ format_list(ch_num, types_with_min_max, "in%s_type==%s") }}"
            },
            "in{{ ch_num }}_max": {
                "title": "Maximum value",
                "group": "g_in{{ ch_num }}",
                "order": 6,
                "address": "0x{{ "%x"|format(ch_num) }}40A",
                "default": 10000,
                "condition": "{{ format_list(ch_num, types_with_min_max, "in%s_type==%s") }}"
            },
            "in{{ ch_num }}_n_type": {
                "title": "Type",
                "address": "0x{{ "%x"|format(ch_num) }}401",
                "group": "gg_in{{ ch_num }}_n",
                "order": 1,
                "condition": "{{ format_list(ch_num, single_ended_modes, "in%s_type==%s") }}",
                "enum": [
