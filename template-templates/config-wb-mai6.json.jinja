                "reg_type": "input",
                "error_value": "0x7fff",
                "format": "s16",
                "address": "0x{{ "%x"|format(ch_num) }}504",
                "group": "g_in{{ ch_num }}",
                "condition": "in{{ ch_num }}_type>=4352&&in{{ ch_num }}_type<=4402||in{{ ch_num }}_type==5889"
            },
            {
                "name": "In {{ch_num}} P State",
                "reg_type": "input",
                "format": "s16",
                "address": "0x{{ "%x"|format(ch_num) }}504",
                "group": "g_in{{ ch_num }}",
                "condition": "in{{ ch_num }}_type==5632"
            },
            {
                "name": "In {{ch_num}} N Resistance",
                "type": "resistance",
                "error_value": "0x7FFFFFFF",
                "reg_type": "input",
                "format": "s32",
                "address": "0x{{ "%x"|format(ch_num) }}502",
                "scale": 0.01,
                "condition": "in{{ ch_num }}_n_type==2||in{{ ch_num }}_n_type==5889||(in{{ ch_num }}_n_type>=4352&&in{{ ch_num }}_n_type<=4402)",
                "group": "gg_in{{ ch_num }}_n"
            },
            {
                "name": "In {{ch_num}} N Temperature",
                "type": "temperature",
