            {
                "name": "In {{ch_num}} N Voltage",
                "type": "value",
                "units": "V",
                "error_value": "0x7FFFFFFF",
                "reg_type": "input",
                "format": "s32",
                "address": "0x{{ "%x"|format(ch_num) }}502",
