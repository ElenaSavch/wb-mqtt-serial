                "required": true,
                "title": "Sampling time",
                "address": "0x{{ "%x"|format(ch_num) }}402",
                "group": "g_in{{ ch_num }}",
                "order": 2,
                "enum_titles": [
                    "50 ms (20 SPS)",
                    "22 ms (45 SPS)",
                    "11 ms (90 SPS)",
                    "5.7 ms (175 SPS)",
                    "3.0 ms (330 SPS)",
                    "1.7 ms (600 SPS)",
                    "1.0 ms (1000 SPS)"
                ],
                "enum": [ 20, 45, 90, 175, 330, 600, 1000 ]
            },
            "in{{ ch_num }}_measurements": {
                "title": "Number of measurements",
                "description": "Total sampling time of channel is (Sampling time * Number of measurements). Channels are polled sequentially.",
