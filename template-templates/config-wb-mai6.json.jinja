        "channels": [
            {% for ch_num in range(1, CHANNELS_NUMBER + 1) -%}
            {
                "name": "In {{ch_num}} Resistance",
                "type": "resistance",
                "error_value": "0x7FFFFFFF",
                "reg_type": "input",
                "format": "s32",
                "address": "0x{{ "%x"|format(ch_num) }}500",
                "scale": 0.01,
                "group": "g_in{{ ch_num }}",
                "condition": "(in{{ ch_num }}_type>=4608&&in{{ ch_num }}_type<=4658)||in{{ ch_num }}_type==258"
            },
            {
                "name": "In {{ch_num}} P Resistance",
                "type": "resistance",
                "error_value": "0x7FFFFFFF",
                "reg_type": "input",
                "format": "s32",
                "address": "0x{{ "%x"|format(ch_num) }}500",
                "scale": 0.01,
                "group": "g_in{{ ch_num }}",
                "condition": "in{{ ch_num }}_type==2||(in{{ ch_num }}_type>=4352&&in{{ ch_num }}_type<=4402)||in{{ ch_num }}_type==5889"
            },
            {
                "name": "In {{ch_num}} Percent",
                "type": "value",
                "units": "%",
