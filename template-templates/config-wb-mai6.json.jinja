                "reg_type": "input",
                "error_value": "0x7fff",
                "format": "s16",
                "address": "0x{{ "%x"|format(ch_num) }}505",
                "condition": "in{{ ch_num }}_n_type==5889||(in{{ ch_num }}_n_type>=4352&&in{{ ch_num }}_n_type<=4402)",
                "group": "gg_in{{ ch_num }}_n"
            },
            {
                "name": "In {{ch_num}} N Percent",
                "type": "value",
                "units": "%",
                "error_value": "0x7FFFFFFF",
                "reg_type": "input",
                "format": "s32",
                "address": "0x{{ "%x"|format(ch_num) }}505",
                "scale": 0.01,
                "group": "gg_in{{ ch_num }}_n",
