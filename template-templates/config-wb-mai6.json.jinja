                "enum_titles": [
                    "2-wire RTD Pt 50 (α = 0.00385 °C⁻¹)",
                    "2-wire RTD Pt 100 (α = 0.00385 °C⁻¹)",
                    "2-wire RTD Pt 500 (α = 0.00385 °C⁻¹)",
                    "2-wire RTD Pt 1000 (α = 0.00385 °C⁻¹)",
                    "2-wire RTD 50P (α = 0.00391 °C⁻¹)",
                    "2-wire RTD 100P (α = 0.00391 °C⁻¹)",
                    "2-wire RTD 500P (α = 0.00391 °C⁻¹)",
                    "2-wire RTD 1000P (α = 0.00391 °C⁻¹)",
                    "2-wire RTD 50M (α = 0.00428 °C⁻¹)",
                    "2-wire RTD 100M (α = 0.00428 °C⁻¹)",
                    "2-wire RTD 500M (α = 0.00428 °C⁻¹)",
                    "2-wire RTD 1000M (α = 0.00428 °C⁻¹)",
                    "2-wire RTD Ni 100 (α = 0.00617 °C⁻¹)",
                    "2-wire RTD Ni 500 (α = 0.00617 °C⁻¹)",
                    "2-wire RTD Ni 1000 (α = 0.00617 °C⁻¹)",

                    "0-5 mA sensor",
                    "0-20 mA sensor",
                    "4-20 mA sensor",

                    "0-30 V sensor",
                    "-50 to 50 mV sensor",

                    "dry contact sensor",
                    "NTC thermistor 10k (B = 3988 K)",

                    "N: voltage measurement from ratiometric sensor",
                    "N: single-ended voltage measurement from 0 to 30 V",
                    "N: two-wire resistance measurement",
                    "N: current measurement"
                ]
            },
            "in{{ ch_num }}_n_data_rate": {
                "required": true,
                "title": "Sampling time",
                "group": "gg_in{{ ch_num }}_n",
                "order": 2,
                "address": "0x{{ "%x"|format(ch_num) }}403",
                "enum_titles": [
                    "50 ms (20 SPS)",
                    "22 ms (45 SPS)",
                    "11 ms (90 SPS)",
                    "5.7 ms (175 SPS)",
                    "3.0 ms (330 SPS)",
                    "1.7 ms (600 SPS)",
                    "1.0 ms (1000 SPS)"
