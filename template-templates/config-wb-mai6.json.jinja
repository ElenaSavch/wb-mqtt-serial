            "in{{ch_num}}_gain": {
                "title": "Gain",
                "group": "g_in{{ ch_num }}",
                "order": 4,
                "address": "0x{{ "%x"|format(ch_num) }}40C",
                "enum": [0, 1, 2, 4],
                "enum_titles": ["auto"],
                "default": 0,
                "condition": "{{ format_list(ch_num, types_with_gain, "in%s_type==%s") }}"
